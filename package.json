{
  "name": "srv-phone-book",
  "version": "0.0.1",
  "description": "Server for phonebook",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --trace-warnings index.js",
    "build:ui": "rm -rf build && cd ../FSO2020_Phonebook && npm run build --prod && cp -r build ../FSO2020_SrvPhB",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m 'UI build' && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "_YA",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0"
  }
}
